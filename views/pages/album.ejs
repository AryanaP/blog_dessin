<html lang="en">
<head>
    <% include ../partials/head %>
</head>

<body class="container">
  <header>
    <% include ../partials/topbar %>
  </header>

  <main>
    <div class="row">

      <div class="col-sm-4">
        <div class="well">
          <h3>Look I'm A Sidebar!</h3>
          <% include ../partials/sidebar %>
        </div>
      </div>

      <div class="col-sm-8">
        <div class="jumbotron">
            <h1> Album </h1>
            <h2>Variable</h2>
            <p><%= tagline %></p>

            <h2>Loop</h2>
            <ul>
              <% drinks.forEach(function(drink) { %>
                <li><%= drink.name %> - <%= drink.drunkness %></li>
              <% }); %>
            </ul>

        </div>
      </div>

    </div>
  </main>

  <footer>
      <% include ../partials/footer %>
  </footer>

</body>
</html>
